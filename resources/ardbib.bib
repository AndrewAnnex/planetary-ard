	Doi = {10.1038/s41597-021-01076-6},
	Id = {Chatenoux2021},
	Isbn = {2052-4463},
	Journal = {Scientific Data},
	Number = {1},
	Pages = {295},
	Title = {The Swiss data cube, analysis ready data archive using earth observations of Switzerland},
	Ty = {JOUR},
